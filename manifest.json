{
    "manifest_version": 3,
  "name": "New App",
  "version": "1.0",
  "description": "Blocks ads on websites",
  "permissions": ["declarativeNetRequest"],
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["Background.js"]
  }],

  "host_permissions": ["<all_urls>"],
  
    "action": {
      "default_popup": "hello.html",
      "default_icon": "hi.png"
    }
  }